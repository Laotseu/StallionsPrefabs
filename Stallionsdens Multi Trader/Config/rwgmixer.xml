<stallionsden>
  <!-- Adds My Museum to the prefab_rule of each group -->
  <append xpath="/rwgmixer/prefab_rules/prefab_rule[@name='commercialGroup']">    
        <prefab name="../../Mods/Stallionsdens Multi Trader/Prefabs/StallionsdensTraderCompound" prob="0.3"/>
    </append>
   <append xpath="/rwgmixer/prefab_rules/prefab_rule[@name='downtownGroup']">    
        <prefab name="../../Mods/Stallionsdens Multi Trader/Prefabs/StallionsdensTraderCompound" prob="0.3"/>
    </append>
   <append xpath="/rwgmixer/prefab_rules/prefab_rule[@name='ruralGroup']">    
        <prefab name="../../Mods/Stallionsdens Multi Trader/Prefabs/StallionsdensTraderCompound" prob="0.3"/>
    </append>
   <append xpath="/rwgmixer/prefab_rules/prefab_rule[@name='wildernessGroup']">    
        <prefab name="../../Mods/Stallionsdens Multi Trader/Prefabs/StallionsdensTraderCompound" prob="0.3"/>
    </append>
   <append xpath="/rwgmixer/prefab_rules/prefab_rule[@name='townGroup']">    
        <prefab name="../../Mods/Stallionsdens Multi Trader/Prefabs/StallionsdensTraderCompound" prob="0.3"/>
    </append>
	<append xpath="/rwgmixer/prefab_rules/prefab_rule[@name='storeGroup']">    
        <prefab name="../../Mods/Stallionsdens Multi Trader/Prefabs/StallionsdensTraderCompound" prob="0.05"/>
	</append>
  </stallionsden>